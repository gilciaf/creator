<Window x:Class="nfecreator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:nfecreator"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None" 
        ResizeMode="NoResize"
        mc:Ignorable="d"
        ShowInTaskbar="False"
        Title="CIAF" Height="125" Width="400">
    <Grid>
        <Grid HorizontalAlignment="Left" Width="400">
            <StackPanel>
                <Image Source="/Pics/Ciaf.png"   Height="100" Width="400"/>
                <TextBox x:Name="NomeTextBox" IsEnabled="False" IsReadOnly="True" HorizontalAlignment="Left" Height="25" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="400"/>
            </StackPanel>
        </Grid>

    </Grid>
</Window>
